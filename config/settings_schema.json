        "definition": [
          {
            "type": "color",
            "id": "background_color",
            "label": "Background Color",
            "default": "#FFFFFF"
          },
          {
            "type": "color",
            "id": "text_color",
            "label": "Text Color",
            "default": "#000000"
          },
          {
            "type": "color",
            "id": "link_color",
            "label": "Link Color",
            "default": "#007BFF"
          },
          {
            "type": "color",
            "id": "button_color",
            "label": "Button Color",
            "default": "#28A745"
          }
        ],
        "role": {
          "text": "background",
          "background": {
            "solid": "background",
            "gradient": "colour_background"
          },
          "links": "background",
          "icons": "background",
          "primary_button": "background",
          "on_primary_button": "background",
          "primary_button_border": "background",
          "secondary_button": "background",
          "on_secondary_button": "background",
          "secondary_button_border": "background"
        }
      }
    ]
  },
