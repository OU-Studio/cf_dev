h1 {
  font-family: "emperaRegular";
  font-size: var(--empera-large-size);
  line-height: var(--empera-large-line-height);
  letter-spacing: var(--empera-large-letter-spacing);
  font-weight: var(--empera-large-weight);
  font-style: var(--empera-large-style);
  color: var(--red);
}

h2 {
  font-family: "garamond-premier-pro-display";
  font-size: var(--garamond-large-size);
  line-height: var(--garamond-large-line-height);
  letter-spacing: var(--garamond-large-letter-spacing);
  font-weight: var(--garamond-large-weight);
  font-style: var(--garamond-large-style);
  color: var(--grey);
}

h3 {
  font-family: "emperaRegular";
  font-size: var(--empera-small-size);
  line-height: var(--empera-small-line-height);
  letter-spacing: var(--empera-small-letter-spacing);
  font-weight: var(--empera-small-weight);
  font-style: var(--empera-small-style);
  color: var(--red);
}

body,
p {
  font-family: "garamond-premier-pro-display";
  font-size: var(--garamond-small-size);
  line-height: var(--garamond-small-line-height);
  letter-spacing: var(--garamond-small-letter-spacing);
  font-weight: var(--garamond-small-weight);
  font-style: var(--garamond-small-style);
  color: var(--grey);
}

.emperaLarge {
  font-family: "emperaRegular" !important;
  font-size: var(--empera-large-size) !important;
  line-height: var(--empera-large-line-height) !important;
  letter-spacing: var(--empera-large-letter-spacing) !important;
  font-weight: var(--empera-large-weight) !important;
  font-style: var(--empera-large-style) !important;
  color: var(--red) !important;
}

.emperaSmall {
  font-family: "emperaRegular" !important;
  font-size: var(--empera-small-size) !important;
  line-height: var(--empera-small-line-height) !important;
  letter-spacing: var(--empera-small-letter-spacing) !important;
  font-weight: var(--empera-small-weight) !important;
  font-style: var(--empera-small-style) !important;
  color: var(--red) !important;
}

.garamondLarge {
  font-family: "garamond-premier-pro-display" !important;
  font-size: var(--garamond-large-size) !important;
  line-height: var(--garamond-large-line-height) !important;
  letter-spacing: var(--garamond-large-letter-spacing) !important;
  font-weight: var(--garamond-large-weight) !important;
  font-style: var(--garamond-large-style) !important;
  color: var(--black) !important;
}

.garamondSmall {
  font-family: "garamond-premier-pro-display" !important;
  font-size: var(--garamond-small-size) !important;
  line-height: var(--garamond-small-line-height) !important;
  letter-spacing: var(--garamond-small-letter-spacing) !important;
  font-weight: var(--garamond-small-weight) !important;
  font-style: var(--garamond-small-style) !important;
  color: var(--black) !important;
}

small,
.garamondTiny {
  font-family: "garamond-premier-pro-display" !important;
  font-size: var(--garamond-tiny-size) !important;
  line-height: var(--garamond-tiny-line-height) !important;
  letter-spacing: var(--garamond-tiny-letter-spacing) !important;
  font-weight: var(--garamond-tiny-weight) !important;
  font-style: var(--garamond-tiny-style) !important;
  color: var(--black) !important;
}

.norwesterLarge {
  font-family: "norwester" !important;
  font-size: var(--norwester-large-size) !important;
  line-height: var(--norwester-large-line-height) !important;
  letter-spacing: var(--norwester-large-letter-spacing) !important;
  font-weight: var(--norwester-large-weight) !important;
  font-style: var(--norwester-large-style) !important;
  color: var(--black) !important;
  text-transform: uppercase !important;
}

.norwesterSmall {
  font-family: "norwester" !important;
  font-size: var(--norwester-small-size) !important;
  line-height: var(--norwester-small-line-height) !important;
  letter-spacing: var(--norwester-small-letter-spacing) !important;
  font-weight: var(--norwester-small-weight) !important;
  font-style: var(--norwester-small-style) !important;
  color: var(--black) !important;
  text-transform: uppercase !important;
}
